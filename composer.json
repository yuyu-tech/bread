{
    "name": "yuyu-tech/bread",
    "description": "It's a Laravel Package to create BREAD APIs using DB configuration.",
    "type": "library",
    "require": {
        "laravel/framework": "^5.8|^6.0"
    },
    "require-dev": {
        "php": "^7.3"
    },
    "license": "YuyuTech",
    "authors": [
        {
            "name": "Bhushan Mer",
            "email": "bhushanmer@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "YuyuTech\\BREAD\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "YuyuTech\\BREAD\\Providers\\BREADServiceProvider"
            ],
            "aliases": {
                "BREAD" : "YuyuTech\\BREAD\\Facades\\BREADFacade"
            }
        }
    }
}
